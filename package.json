{
  "name": "random-weathers",
  "version": "1.0.0",
  "description": "A TypeScript Express app that returns random weather for cities around the world using OpenStreetMap Nominatim and Open-Meteo APIs",
  "main": "dist/server.js",
  "scripts": {
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server.js",
    "dev:server": "ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "install:all": "npm install && cd client && npm install"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "weather",
    "express",
    "typescript",
    "open-meteo",
    "nominatim",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
